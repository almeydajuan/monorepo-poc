name: User
on:
  push:
    paths:
      - user/**
    branches:
      - main
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout
        uses: actions/checkout@v4.1.7
        with:
          fetch-depth: 1
          persist-credentials: false
      - name: Setup JVM
        uses: actions/setup-java@v4.3.0
        with:
          distribution: temurin
          java-version: 21
          cache: gradle
